      "@ty-swap-pages/page-custom-fields": ["./src/packages/page-custom-fields/src/index"],
      "@ty-swap-pages/page-swap-position-list": [
        "./src/packages/page-swap-position-list/src/index"
      ],
      "@ty-swap-pages/page-swap-contract-source-manage": [
        "./src/packages/page-swap-contract-source-manage/src/index"
      ],
      "@ty-swap-pages/page-sales-manage": ["./src/packages/page-sales-manage/src/index"],

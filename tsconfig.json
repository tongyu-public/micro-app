      "@ty-swap-pages/page-field-precision-manage": [
        "./src/packages/page-field-precision-manage/src/index"
      ],
      "@ty-swap-pages/helpers": ["./src/packages/helpers/src/index"]
    }
  },
  "include": [
    "mock/**/*",
    "src/**/*",
    "playwright.config.ts",
    "tests/**/*",
    "test/**/*",
    "__test__/**/*",
    "typings/**/*",
    "config/**/*",
    ".eslintrc.js",
    ".stylelintrc.js",
    ".prettierrc.js",
    "jest.config.js",
    "mock/*",
    "typings.d.ts"
  ],
  "exclude": ["node_modules", "build", "dist", "scripts", "src/.umi/*", "webpack", "jest"]
}

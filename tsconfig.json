{
  "compilerOptions": {
    "outDir": "build/dist",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "baseUrl": ".",
    "jsx": "preserve",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "allowJs": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./src/.umi/*"],
      "@ty-swap-pages/utils": ["./src/packages/utils/src/index"],
      "@ty-swap-pages/base": ["./src/packages/base/src/index"],
      "@ty-swap-pages/provider": ["./src/packages/provider/src/index"],
      "@ty-swap-pages/io-component": ["./src/packages/io-component/src/index"],
      "@ty-swap-pages/services": ["./src/packages/services/src/index"],
      "@ty-swap-pages/fields": ["./src/packages/fields/src/index"],
      "@ty-swap-pages/page-swap-booking": ["./src/packages/page-swap-booking/src/index"],
      "@ty-swap-pages/page-swap-position-detail": [
        "./src/packages/page-swap-position-detail/src/index"
      ],
      "@ty-swap-pages/core": ["./src/packages/core/src/index"],
      "@ty-swap-pages/services-swap": ["./src/packages/services-swap/src/index"],
      "@ty-swap-pages/page-source-manage": ["./src/packages/page-source-manage/src/index"],
      "@ty-swap-pages/page-one-click-execution": [
        "./src/packages/page-one-click-execution/src/index"
      ],
      "@ty-swap-pages/containers": ["./src/packages/containers/src/index"],
      "@ty-swap-pages/design-operations": ["./src/packages/design-operations/src/index"],
      "@ty-swap-pages/services-option": ["./src/packages/services-option/src/index"],
      "@ty-swap-pages/global": ["./src/packages/global/src/index"],
      "@ty-swap-pages/page-frame-valuation-report": [
        "./src/packages/page-frame-valuation-report/src/index"
      ],
      "@ty-swap-pages/page-swap-contract-detail-report": [
        "./src/packages/page-swap-contract-detail-report/src/index"
      ],
      "@ty-swap-pages/page-pnl-reports": ["./src/packages/page-pnl-reports/src/index"],
      "@ty-swap-pages/page-risk-reports": ["./src/packages/page-risk-reports/src/index"],

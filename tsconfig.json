      "@ty-swap-pages/page-swap-dividend-management": [
        "./src/packages/page-swap-dividend-management/src/index"
      ],
      "@ty-swap-pages/page-swap-margin-manage": [
        "./src/packages/page-swap-margin-manage/src/index"
      ],
      "@ty-swap-pages/page-system-settings": ["./src/packages/page-system-settings/src/index"],
      "@ty-swap-pages/page-valuation-management": [
        "./src/packages/page-valuation-management/src/index"
      ],
      "@ty-swap-pages/page-water-recording-management": [
        "./src/packages/page-water-recording-management/src/index"
      ],
      "@ty-swap-pages/page-swap-submitted": ["./src/packages/page-swap-submitted/src/index"],

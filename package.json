{
  "name": "liquidation-admin",
  "private": true,
  "scripts": {
    "clean:link": "yarn unlink @ty-one-start/one-start && yarn unlink @ty-one-start/request && yarn install --force --registry=http://npm.tongyu.tech",
    "deploy:dev": "npm run build && npm run deploy:dev:only",
    "deploy:dev2": "npm run build && npm run deploy:dev2:only",
    "deploy:dev3": "npm run build && npm run deploy:dev3:only",
    "deploy:test": "npm run build && npm run deploy:test:only",
    "deploy:dev:only": "cross-env DEPLOY_ENV=dev node ./scripts/deploy.js",
    "deploy:dev2:only": "cross-env DEPLOY_ENV=dev2 node ./scripts/deploy.js",
    "deploy:dev3:only": "cross-env DEPLOY_ENV=dev3 node ./scripts/deploy.js",
    "deploy:test:only": "cross-env DEPLOY_ENV=test node ./scripts/deploy.js",
    "merge-packages-pkgs": "node ./scripts/merge-packages-pkgs.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build && yarn gen:qiankun-entry",
    "dev": "npm run start:dev",
    "format": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "ci": "yarn lint && yarn tsc",
    "lint": "npm run lint:formats && npm run lint:code && npm run lint:style",
    "lint:staged": "lint-staged",
    "lint:formats": "prettier --version && prettier -c \"src/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "lint:commit": "commitlint -E HUSKY_GIT_PARAMS",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --syntax less --fix ./src/**/**.less",
    "clean": "rimraf dist lib",
    "gen:api": "cross-env OPEN_API=true umi openapi && yarn clean:api-index",
    "clean:api-index": "rimraf ./src/services/*/index.ts",
    "precommit": "lint-staged",
    "prepush": "yarn tsc",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "yarn start:dev",
    "start:dev": "cross-env PORT=8066 APP_ENV=dev UMI_ENV=dev MOCK=none umi dev",
    "start:test": "cross-env APP_ENV=dev UMI_ENV=dev MOCK=none umi dev",
    "test": "umi test",
    "test:e2e": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "gen:qiankun-entry": "node ./scripts/gen-qiankun-entry.js",
    "prepare": "husky install"
  },
